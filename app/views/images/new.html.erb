<%= form_for @image do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.label :photographer %>
  <%= f.text_field :photographer %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit 'Add image!' %>
<% end %>
   